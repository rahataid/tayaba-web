{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@constants": ["src/constants"],
      "@utils/*": ["src/utils/*"],
      "@routes/*": ["src/routes/*"],
      "@layouts/*": ["src/layouts/*"],
      "@services/*": ["src/services/*"],
      "@services": ["src/services"],
      "@guards/*": ["src/guards/*"],
      "@contexts/*": ["src/contexts/*"],
      "@hooks/*": ["src/hooks/*"],
      "@web3/*": ["src/web3/*"],
      "@sections/*": ["src/sections/*"],
      "@config": ["src/config.js"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
